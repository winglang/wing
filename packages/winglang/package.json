{
  "name": "winglang",
  "version": "0.0.0",
  "description": "Wing Programming Language",
  "files": [
    "wingc.wasm",
    "bin/wing",
    "dist/",
    "project-templates/"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=v18.0.0"
  },
  "main": "./dist/cli.js",
  "bin": {
    "wing": "./bin/wing"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/winglang/wing.git"
  },
  "scripts": {
    "build": "tsc && pnpm copy-root-readme",
    "eslint": "eslint --fix --ext .ts src",
    "compile": "tsc",
    "copy-root-readme": "cp ../../README.md ./README.md && pnpm copy-root-assets",
    "copy-root-assets": "cp ../../logo/demo.gif ./logo/demo.gif",
    "watch": "tsc --watch",
    "test": "vitest run --update",
    "dev": "./bin/wing",
    "package": "bump-pack -b"
  },
  "dependencies": {
    "@npmcli/arborist": "^7.5.3",
    "@segment/analytics-node": "^1.3.0",
    "@supercharge/promise-pool": "^3.2.0",
    "@wingconsole/app": "workspace:^",
    "@wingconsole/server": "workspace:^",
    "@winglang/compiler": "workspace:^",
    "@winglang/sdk": "workspace:^",
    "chalk": "^4.1.2",
    "codespan-wasm": "0.4.0",
    "commander": "^10.0.1",
    "compare-versions": "^5.0.3",
    "debug": "^4.3.5",
    "glob": "^10.4.2",
    "inquirer": "^8.2.6",
    "nanoid": "^3.3.7",
    "npm-packlist": "^8.0.2",
    "open": "^8.4.2",
    "ora": "^5.4.1",
    "tar": "^6.2.1",
    "tiny-updater": "^3.5.2",
    "uuid": "^8.3.2",
    "vscode-languageserver": "^8.1.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.11.0",
    "@types/node-persist": "^3.1.8",
    "@types/npm-packlist": "^7.0.3",
    "@types/npmcli__arborist": "^5.6.9",
    "@types/semver-utils": "^1.1.3",
    "@types/tar": "^6.1.13",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "bump-pack": "workspace:^",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^4.2.1",
    "typescript": "^5.5.2",
    "vitest": "^1.6.0"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
