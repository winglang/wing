{
  "$schema": "https://turborepo.org/schema.json",
  "extends": ["//"],
  "pipeline": {
    "compile": {
      "outputs": ["lib/**"]
    },
    "dev": {
      "dependsOn": ["compile"],
      "cache": false,
      "persistent": true
    },
    "package": {
      "inputs": ["syntaxes/**", "resources/**"],
      "outputs": ["../../dist/vscode-wing.vsix"]
    }
  }
}
