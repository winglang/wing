{
  "name": "console-server",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "implicitDependencies": ["compiler", "sdk"],
  "targets": {
    "build": {
      "options": {
        "command": "pnpm run compile",
        "cwd": "apps/wing-console/console/server"
      }
    },
    "test": {
      "options": {
        "command": "pnpm run vitest",
        "cwd": "apps/wing-console/console/server"
      }
    },
    "package": {
      "options": {
        "command": "pnpm run package",
        "cwd": "apps/wing-console/console/server"
      }
    }
  }
}
