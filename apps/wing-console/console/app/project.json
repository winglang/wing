{
  "name": "console-app",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "implicitDependencies": ["sdk", "console-server"],
  "targets": {
    "build": {
      "options": {
        "command": "pnpm compile",
        "cwd": "apps/wing-console/console/app"
      }
    },
    "test": {
      "dependsOn": ["build"],
      "options": {
        "command": "pnpm run test",
        "cwd": "apps/wing-console/console/app"
      }
    },
    "package": {
      "options": {
        "command": "pnpm package",
        "cwd": "apps/wing-console/console/app"
      }
    }
  }
}
