{
  "$schema": "https://turborepo.org/schema.json",
  "extends": ["//"],
  "tasks": {
    "dev": {
      "dependsOn": ["^compile"]
    },
    "compile": {
      "outputs": ["dist/**"],
      "env": ["SEGMENT_WRITE_KEY", "VITE_WING_CLOUD_SIGN_IN_URL"]
    },
    "package": {
      "outputs": ["../../../../dist/wingconsole-app-*.tgz"]
    },
    "preview:fly": {
      "dependsOn": ["compile"],
      "inputs": ["demo/*", "preview/*"]
    }
  }
}
