{
  "name": "wing",
  "root": "apps/wing/",
  "sourceRoot": "apps/wing",
  "projectType": "application",
  "implicitDependencies": [
    "wingc"
  ],
  "targets": {
    "get-wingc": {
      "executor": "nx:run-commands",
      "inputs": ["rust-wasm"],
      "dependsOn": ["^build-wasm"],
      "options": {
        "command": "cp ../../target/wasm32-wasi/debug/wingc.wasm ./",
        "cwd": "apps/wing"
      },
      "configurations": {
        "release": {
          "command": "cp ../../target/wasm32-wasi/release/wingc.wasm ./"
        }
      }
    },
    "build": {
      "executor": "nx:run-script",
      "inputs": ["src"],
      "outputs": ["dist"],
      "dependsOn": ["^build", "get-wingc"],
      "options": {
        "cwd": "apps/wing",
        "script": "build"
      }
    }
  }
}