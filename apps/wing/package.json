{
  "name": "winglang",
  "version": "0.0.0",
  "description": "Wing Programming Language",
  "files": [
    "wingc.wasm",
    "bin/wing",
    "dist/"
  ],
  "engines": {
    "node": ">=v18.0.0"
  },
  "main": "./dist/index.js",
  "bin": {
    "wing": "./bin/wing"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/winglang/wing.git"
  },
  "scripts": {
    "copy-wingc-debug": "cp ../../target/wasm32-wasi/debug/wingc.wasm ./",
