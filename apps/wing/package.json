  "dependencies": {
    "@winglang/sdk": "file:../../libs/wingsdk",
    "chalk": "^4.1.2",
    "commander": "^10.0.0",
    "compare-versions": "^5.0.3",
    "debug": "^4.3.4",
    "is-installed-globally": "^0.4.0",
    "open": "^8.4.0",
    "update-notifier": "^6.0.2",
    "vscode-languageserver": "^8.0.2",
    "wasi-js": "^1.7.3"
  },
  "bundledDependencies": [
    "wasi-js"
  ],
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/jest": "^29.2.6",
    "@types/node": "^18.11.18",
    "@types/node-persist": "^3.1.3",
    "@types/semver-utils": "^1.1.1",
    "@types/update-notifier": "^6.0.1",
    "jest": "^29.3.1",
    "patch-package": "^6.5.1",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.4"
  }
