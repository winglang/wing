{
  "name": "winglang",
  "version": "0.0.0",
  "description": "Wing Programming Language",
  "files": [
    "wingc.wasm",
    "bin/wing",
    "dist/"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=v18.0.0"
  },
  "main": "./dist/cli.js",
  "bin": {
    "wing": "./bin/wing"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/winglang/wing.git"
  },
  "scripts": {
    "build": "tsc && pnpm copy-root-readme",
    "copy-root-readme": "cp ../../README.md ./README.md && pnpm copy-root-assets",
    "copy-root-assets": "cp ../../logo/demo.gif ./logo/demo.gif",
    "watch": "tsc --watch",
    "test": "vitest run",
    "dev": "pnpm build && ./bin/wing",
    "package": "bump-pack -b"
  },
  "dependencies": {
    "@segment/analytics-node": "^1.0.0",
    "@wingconsole/app": "workspace:^",
    "@winglang/compiler": "workspace:^",
    "@winglang/sdk": "workspace:^",
    "chalk": "^4.1.2",
    "codespan-wasm": "0.4.0",
    "commander": "^10.0.0",
    "compare-versions": "^5.0.3",
    "debug": "^4.3.4",
    "open": "^8.4.0",
    "ora": "^5.4.1",
    "tiny-updater": "^3.4.0",
    "uuid": "^8.3.2",
    "vscode-languageserver": "^8.0.2"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/node": "^18.11.18",
    "@types/node-persist": "^3.1.3",
    "@types/semver-utils": "^1.1.1",
    "@types/update-notifier": "^6.0.1",
    "@types/uuid": "^8.3.2",
    "bump-pack": "workspace:^",
    "esbuild": "^0.17.19",
    "typescript": "^4.9.4",
    "vitest": "^0.30.1"
  },
  "volta": {
    "extends": "../../package.json"
  }
}