{
  "name": "winglang",
  "version": "0.0.0",
  "description": "Wing Programming Language",
  "files": [
    "wingc.wasm",
    "bin/wing",
    "dist/"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=v18.0.0"
  },
  "main": "./dist/cli.js",
  "bin": {
    "wing": "./bin/wing"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/winglang/wing.git"
  },
  "scripts": {
    "build": "tsc && pnpm copy-root-readme",
    "eslint": "eslint --fix --ext .ts src",
    "compile": "tsc",
    "copy-root-readme": "cp ../../README.md ./README.md && pnpm copy-root-assets",
    "copy-root-assets": "cp ../../logo/demo.gif ./logo/demo.gif",
    "watch": "tsc --watch",
    "test": "vitest run",
    "dev": "./bin/wing",
    "package": "bump-pack -b"
  },
  "dependencies": {
    "@npmcli/arborist": "^7.2.0",
    "@segment/analytics-node": "^1.1.0",
    "@wingconsole/app": "workspace:^",
    "@wingconsole/server": "workspace:^",
    "@winglang/compiler": "workspace:^",
    "@winglang/sdk": "workspace:^",
    "chalk": "^4.1.2",
    "codespan-wasm": "0.4.0",
    "commander": "^10.0.1",
    "compare-versions": "^5.0.3",
    "debug": "^4.3.4",
    "glob": "^10.3.10",
    "nanoid": "^3.3.6",
    "npm-packlist": "^8.0.0",
    "open": "^8.4.2",
    "ora": "^5.4.1",
    "tar": "^6.2.0",
    "tiny-updater": "^3.5.1",
    "uuid": "^8.3.2",
    "vscode-languageserver": "^8.1.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.8",
    "@types/node": "^18.17.13",
    "@types/node-persist": "^3.1.4",
    "@types/npm-packlist": "^7.0.1",
    "@types/npmcli__arborist": "^5.6.2",
    "@types/semver-utils": "^1.1.1",
    "@types/tar": "^6.1.6",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "bump-pack": "workspace:^",
    "esbuild": "^0.17.19",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^4.2.1",
    "typescript": "^4.9.5",
    "vitest": "^0.30.1"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
