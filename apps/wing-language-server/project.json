{
  "name": "wing-language-server",
  "root": "apps/wing-language-server/",
  "sourceRoot": "apps/wing-language-server",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "inputs": ["src", "rust-conf"],
      "outputs": ["rust-native"],
      "dependsOn": ["^build"],
      "options": {
          "command": "cargo build",
          "cwd": "libs/wingc"
      },
      "configurations": {
        "release": {
          "command": "cargo build --release"
        }
      }
    },
    "test": {
      "executor": "@nxrs/cargo:test",
      "inputs": ["src", "rust-conf"],
      "dependsOn": ["build"]
    }
  }
}
