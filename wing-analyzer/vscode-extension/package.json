{
  "name": "wing",
  "displayName": "Wing [Alpha]",
  "description": "Wing Language Support ",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "scripts": {
    "bundle": "esbuild ./src/extension.ts --outfile=lib/extension.js --external:node-gyp --external:vscode --format=cjs --platform=node --bundle"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "wing",
        "aliases": [
          "Wing",
          "wing"
        ],
        "extensions": [
          ".w"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "resources/logo.png",
          "dark": "resources/logo.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "wing",
        "scopeName": "source.wing",
        "path": "./syntaxes/wing.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "esbuild": "^0.14.54"
  }
}
