{
  "name": "wing",
  "displayName": "Wing [Alpha]",
  "description": "Wing Language Support ",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/monadahq/winglang.git"
  },
  "scripts": {
    "bundle": "esbuild src/extension.ts --outfile=lib/index.js --external:node-gyp --external:vscode --format=cjs --platform=node --bundle",
    "package": "npm run bundle && vsce package -o vscode-wing.vsix"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:wing"
  ],
  "publisher": "Monada",
  "main": "lib/index.js",
  "icon": "resources/logo.png",
  "contributes": {
    "languages": [
      {
        "id": "wing",
        "aliases": [
          "Wing",
          "wing"
        ],
        "extensions": [
          ".w"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "resources/logo.png",
          "dark": "resources/logo.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "wing",
        "scopeName": "source.wing",
        "path": "./syntaxes/wing.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "esbuild": "^0.14.54",
    "vsce": "^2.10.0"
  }
}
