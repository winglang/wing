{
  "name": "tree-sitter-wing",
  "root": "libs/tree-sitter-wing/",
  "sourceRoot": "libs/tree-sitter-wing",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "inputs": ["{projectRoot}/grammar.js"],
      "outputs": ["src"],
      "dependsOn": ["^build"],
      "options": {
          "command": "cargo build",
          "cwd": "libs/tree-sitter-wing"
      },
      "configurations": {
        "release": {
          "command": "cargo build --release"
        }
      }
    },
    "test": {
      "executor": "@nxrs/cargo:test",
      "inputs": ["src", "rust-conf"],
      "dependsOn": ["build"]
    }
  }
}
