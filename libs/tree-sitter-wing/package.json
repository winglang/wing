{
  "name": "@winglang/tree-sitter-wing",
  "version": "0.0.0",
  "description": "winglang grammar for tree-sitter",
  "main": "bindings/node",
  "keywords": [
    "parsing",
    "incremental"
  ],
  "tree-sitter": [
    {
      "scope": "source.wing",
      "file-types": [
        "w"
      ]
    }
  ],
  "dependencies": {
    "nan": "^2.12.1"
  },
  "scripts": {
    "test": "npx tree-sitter-cli test",
    "test:update": "npx tree-sitter-cli test --update"
  },
  "volta": {
    "node": "18.12.1",
    "npm": "8.19.3"
  }
}
