{
  "name": "@winglang/tree-sitter-wing",
  "version": "0.0.0",
  "description": "winglang grammar for tree-sitter",
  "main": "bindings/node",
  "keywords": [
    "parsing",
    "incremental"
  ],
  "tree-sitter": [
    {
      "scope": "source.wing",
      "file-types": [
        "w"
      ]
    }
  ],
  "dependencies": {
    "nan": "^2.12.1"
  },
  "scripts": {
    "test": "tree-sitter test",
    "test:update": "tree-sitter test --update",
    "test:watch": "nodemon --watch grammar.js --watch src/scanner.c --exec \"pnpm run test\"",
    "build": "tree-sitter generate",
    "build:watch": "nodemon --watch grammar.js --watch src/scanner.c --exec \"pnpm run build\"",
    "build:wasm": "pnpm run build && tree-sitter build-wasm --docker",
    "playground": "pnpm run build:wasm && tree-sitter playground",
    "playground:quiet": "pnpm run build:wasm && tree-sitter playground -q",
    "playground:watch": "nodemon --watch grammar.js --watch src/scanner.c --exec \"pnpm run playground:quiet\""
  },
  "volta": {
    "node": "18.16.0",
    "pnpm": "8.6.5"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "tree-sitter-cli": "0.20.6"
  }
}
