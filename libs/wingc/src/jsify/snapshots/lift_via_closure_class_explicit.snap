    class MyClosure {
      q: cloud.Queue;
      init() {
        this.q = new cloud.Queue();
      }

      inflight handle() {
        this.another();
      }

      inflight another() {
        this.q.push("hello");
      }
    }
    
    let fn = new MyClosure();
    
    test "test" {
