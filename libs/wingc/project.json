{
  "name": "wingc",
  "root": "libs/wingc/",
  "sourceRoot": "libs/wingc/src",
  "projectType": "library",
  "implicitDependencies": ["tree-sitter-wing", "wingii"],
  "targets": {
    "build-native": {
      "executor": "nx:run-commands",
      "inputs": ["src", "rust-conf"],
      "outputs": ["rust-native"],
      "dependsOn": ["^build"],
      "options": {
          "command": "cargo build",
          "cwd": "libs/wingc"
      },
      "configurations": {
        "production": {
          "options": {
            "command": "cargo build --release"
          }
        }
      }
    },
    "test": {
      "executor": "@nxrs/cargo:test",
      "inputs": ["src", "rust-conf"],
      "dependsOn": ["build"]
    },
    "build-wasm": {
      "executor": "nx:run-commands",
      "inputs": ["src", "rust-conf"],
      "outputs": ["rust-wasm"],
      "dependsOn": ["build"],
      "options": {
          "command": "cargo wasi build",
          "cwd": "libs/wingc"
      },
      "configurations": {
        "production": {
          "options": {
            "command": "cargo wasi build --release"
          }
        }
      }
    }
  }
}
