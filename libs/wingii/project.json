{
  "name": "@monadahq/wingii",
  "root": "libs/wingii/",
  "sourceRoot": "libs/wingii/src",
  "projectType": "library",
  "targets": {
    "build-native": {
      "executor": "@nxrs/cargo:build",
      "inputs": ["src", "rust-conf"],
      "outputs": ["rust-native"],
      "dependsOn": ["^build"]
    },
    "test": {
      "executor": "@nxrs/cargo:test",
      "inputs": ["src", "rust-conf"],
      "dependsOn": ["build"]
    },
    "build-wasm": {
      "executor": "@nxrs/cargo:build",
      "inputs": ["src", "rust-conf"],
      "outputs": ["rust-wasm"],
      "dependsOn": ["^build"]
    }
  }
}
