{
  "$schema": "https://turborepo.org/schema.json",
  "extends": ["//"],
  "tasks": {
    "compile": {
      "outputs": ["dist/**", "lib/**"]
    },
    "test": {
      "dependsOn": ["compile"]
    },
    "package": {
      "dependsOn": ["compile"],
      "outputs": ["../../dist/winglang-platform-awscdk-*.tgz"]
    }
  }
}
