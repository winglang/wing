{
  "$schema": "https://turborepo.org/schema.json",
  "extends": ["//"],
  "pipeline": {
    "pre-compile": {
      "inputs": ["cdktf.json"],
      "outputs": ["src/.gen/**"]
    },
    "compile": {
      "dependsOn": ["pre-compile"],
      "outputs": ["lib/**", ".jsii"]
    },
    "post-compile": {
      "dependsOn": ["compile"],
      "inputs": [".jsii", "src/**/*.md"],
      "outputs": ["../../docs/docs/04-standard-library/*/*.md"]
    },
    "eslint": {
      "dependsOn": ["pre-compile"]
    },
    "test": {
      "dependsOn": ["pre-compile"]
    },
    "package": {
      "outputs": ["../../dist/winglang-sdk-*.tgz"]
    }
  }
}
