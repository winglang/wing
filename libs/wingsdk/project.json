{
  "name": "sdk",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "implicitDependencies": ["wing-api-checker", "jsii-docgen"],
  "targets": {
    "build": {
      "options": {
        "commands": [
          "pnpm projen",
          "pnpm pre-compile",
          "pnpm compile"
        ],
        "cwd": "libs/wingsdk",
        "parallel": false
      }
    },
    "test": {
      "dependsOn": ["^build", "build"],
      "options": {
        "commands": ["pnpm post-compile", "pnpm test"],
        "cwd": "libs/wingsdk",
        "parallel": false
      }
    },
    "package": {
      "options": {
        "commands": ["pnpm bump", "pnpm pack", "pnpm unbump"],
        "cwd": "libs/wingsdk",
        "parallel": false
      }
    }
  }
}
