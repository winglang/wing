{
  "name": "wingsdk",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "implicitDependencies": [
    "wing-api-checker"
  ],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run build",
        "cwd": "libs/wingsdk"
      }
    },
    "package": {
      "dependsOn": [
        "build"
      ],
      "executor": "nx:run-commands",
      "options": {
        "commands": ["npm run bump", "npm pack", "npm run unbump"],
        "cwd": "libs/wingsdk",
        "parallel": false
      }
    }
  }
}
