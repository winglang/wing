{
  "name": "sdk",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "implicitDependencies": ["wing-api-checker", "jsii-docgen"],
  "targets": {
    "build": {
      "options": {
        "commands": [
          "pnpm run projen",
          "pnpm run pre-compile",
          "pnpm run compile"
        ],
        "cwd": "libs/wingsdk",
        "parallel": false
      }
    },
    "test": {
      "dependsOn": ["^build", "build"],
      "options": {
        "commands": ["pnpm run post-compile", "pnpm run test"],
        "cwd": "libs/wingsdk",
        "parallel": false
      }
    },
    "package": {
      "options": {
        "commands": ["pnpm run bump", "pnpm pack", "pnpm run unbump"],
        "cwd": "libs/wingsdk",
        "parallel": false
      }
    }
  }
}
