{
  "name": "hangar",
  "implicitDependencies": [
    "winglang",
    "sdk"
  ],
  "targets": {
    "generate": {
      "executor": "nx:run-commands",
      "inputs": [
        "src",
        "examples-tests"
      ],
      "outputs": [
        "{projectRoot}/src/generated_tests/**/*}"
      ],
      "options": {
        "command": "npm run test:generate",
        "cwd": "tools/hangar"
      }
    },
    "test": {
      "dependsOn": [
        "^build",
        "^package",
        "generate"
      ],
      "inputs": [
        "src",
        "examples-tests"
      ],
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run test:update",
        "cwd": "tools/hangar"
      },
      "configurations": {
        "release": {
          "command": "npm run test"
        }
      }
    }
  }
}
